                  <div style="margin-left:25px;">
                     <script type="text/javascript">
            if(DEBUG.enabled()) alert(
            "myFQN:         lat\n" +
            "myJSVarName:   org_opendap_lat\n" +
            "checkBoxName:  get_org_opendap_lat\n" +
            "isContainer:   false\n" +
            "isArray:       true\n" +
            "(parent) container: \n"
            );

            org_opendap_lat = new dap_var("lat", "org_opendap_lat", true,false);

            
                DAP4_URL.add_dap_var(org_opendap_lat);
            org_opendap_lat.checkBox = "get_org_opendap_lat";
            </script>
                     <div style="color: black;margin-left:-20px;margin-top:10px">
                        <input type="checkbox" id="get_org_opendap_lat"
                               onclick="org_opendap_lat.handle_projection_change(get_org_opendap_lat)"
