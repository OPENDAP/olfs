                <span class="small" style="vertical-align: 15%;">lat=
                    </span>
                0
                <span class="medium" style="vertical-align: 10%;">..</span>16]
            </span>
                        <span class="small" style="vertical-align: 15%; font-size: 25%;">(Type is Float32)
            </span>
                     </div>
                     <input type="text" id="org_opendap_lat_dim_1" size="8" oninput="autoResize(event)"
                            onfocus="describe_index()"
                            onChange="DAP4_URL.update_url()"/>
                     <script type="text/javascript">org_opendap_lat.addDimension(org_opendap_lat_dim_1,17);
            </script>
